<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">All Order Id in System Mangento</argument>
            </action>
        </referenceBlock>
        <referenceBlock name="catalog.compare.sidebar" remove="true"/>
        <referenceBlock name="wishlist_sidebar" remove="true"/>
        <referenceBlock name="navigation.sections" remove="true"/>
        <referenceContainer name="content">
            <block class="Magenest\Questioning\Block\Questioning\ShowOrderID" name="show.order.id" template="Magenest_Questioning::showorderid.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
